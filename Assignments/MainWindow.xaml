<Window x:Class="Assignments.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignments"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Search with Google Api" Height="650" Width="900">
    <Grid>
        <TextBox Name="SearchQueryTextBox" Width="300" Margin="10,65,583.6,517"/>
        <Button Name="SearchButton" Content="Search" Width="100" Margin="328,65,465.6,517" Click="SearchButton_Click"/>
        <DataGrid Name="SearchResultsDataGrid" Margin="10,108,9.6,10">

        </DataGrid>
        <Label Content="Search Data from Google" HorizontalAlignment="Left" Margin="10,18,0,0" VerticalAlignment="Top" Height="41" Width="300" FontSize="20"/>
        <Button x:Name="ClearButton" Content="Clear" Width="100" Margin="564,65,229.6,517" Click="ClearButton_Click"/>
        <Button x:Name="HistoryButton" Content="Load History" Width="100" Margin="742,65,51.6,517" Click="HistoryButton_Click"/>
        <Button x:Name="SaveSearchResult" Content="SaveSearchResult" Width="100" Margin="448,65,345.6,517" Click="SaveSearchResult_Click"/>


    </Grid>
</Window>
