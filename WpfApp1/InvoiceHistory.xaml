<Window x:Class="WpfApp1.InvoiceHistory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="InvoiceHistory" Height="450" Width="800">
    <Grid>
        <Label Content="Old Invoices" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" HorizontalContentAlignment="Center" Width="784"/>
        <DataGrid HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="784" Margin="0,37,0,0" Name="gridInvoices" MouseDoubleClick="GridInvoices_MouseDoubleClick"/>
        
    </Grid>
</Window>
